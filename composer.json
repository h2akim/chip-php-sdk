{
    "name": "hakimrazalan/chip-php-sdk",
    "description": "PHP framework agnostic to work with CHIP Asia API",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Chip\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Chip\\Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "Hakim Razalan",
            "email": "hakim.razalan@gmail.com"
        }
    ],
    "require": {
        "php": "^7.4 || ^8.0",
        "json-mapper/json-mapper": "^2.22",
        "symfony/http-client": "^5.4",
        "laravie/codex": "^5.0",
        "laravie/codex-filter": "^1.0"
    },
    "config": {
        "allow-plugins": {
            "php-http/discovery": true
        }
    },
    "require-dev": {
        "php-http/guzzle7-adapter": "^1.1",
        "phpunit/phpunit": "^9.6"
    },
    "repositories": {
        "laravie/codex": {
            "type": "vcs",
            "url": "git@github.com:h2akim/codex.git"
        },
        "laravie/codex-filter": {
            "type": "vcs",
            "url": "git@github.com:h2akim/codex-filter.git"
        },
        "laravie/codex-security": {
            "type": "vcs",
            "url": "git@github.com:h2akim/codex-security.git"
        },
        "laravie/codex-common": {
            "type": "vcs",
            "url": "git@github.com:h2akim/codex-common.git"
        }
    }
}
